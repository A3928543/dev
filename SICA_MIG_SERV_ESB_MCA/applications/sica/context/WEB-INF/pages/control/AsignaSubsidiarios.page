<!DOCTYPE page-specification PUBLIC
  "-//Apache Software Foundation//Tapestry Specification 3.0//EN"
  "http://jakarta.apache.org/tapestry/dtd/Tapestry_3_0.dtd">

<page-specification class="com.ixe.ods.sica.pages.control.AsignaSubsidiarios">
    <bean name="delegate" class="com.ixe.isis.tapestry.IxenetValidationDelegate"/>
    <bean name="evenOdd" class="org.apache.tapestry.bean.EvenOdd"/>
    
	<property-specification name="razonSocial" type="java.lang.String" initial-value="" />
    <property-specification name="noCuenta" type="java.lang.String" initial-value="" />
    <property-specification name="nombreCorto" type="java.lang.String" initial-value="" persistent="yes" />
    <property-specification name="clientes" type="java.util.List" persistent="yes" initial-value="new java.util.ArrayList()" />
    <property-specification name="subsidiarios" type="java.util.List" persistent="yes" initial-value="new java.util.ArrayList()" />
    <property-specification name="asignados" type="java.util.List" persistent="yes" initial-value="new java.util.ArrayList()" />
    <property-specification name="idCorporativo" type="java.lang.Integer" persistent="yes" />
    <property-specification name="modoRefresh" type="int" />	
    <property-specification name="operacionExitosa" type="boolean" persistent="yes"/>
	
    <context-asset name="searchButton" path="/images/search.gif" />
    <context-asset name="cancelarButton" path="/images/cancelar_todo.gif" />
</page-specification>    
